<% if (rooms.length === 0) { %>
<h2> You don't have any notes just yet </h2>

<a href="/rooms/new"> Create a New Note
</a>

<% } else { %>
<h2> Your Notes </h2>

<% if (successMessage.length > 0) { %>
<div class="success"> <%= successMessage %> </div>
<% } %>

<ul>
  <% rooms.forEach((oneRoom) => { %>
  <li>
    <h3> <%= oneRoom.name %> </h3>
    <p> <%= oneRoom.description %> </p>
    <img src="<%= oneRoom.photoAddress %>">
</li>
<%  }) %>
</ul>

<% } %>
